<?xml version="1.0" encoding="us-ascii"?>
<Loqui xmlns="http://tempuri.org/LoquiSource.xsd">
  <Object name="Attack" objType="Subrecord">
    <Fields>
      <RefDirect name="AttackData" refName="AttackData" />
      <String name="AttackEvent" recordType="ATKE" />
      <FormLink name="WeaponSlot" refName="EquipType" recordType="ATKW" />
      <FormLink name="RequiredSlot" refName="EquipType" recordType="ATKS" />
      <String name="Description" recordType="ATKT" />
    </Fields>
  </Object>
  <Object name="AttackData" objType="Subrecord" recordType="ATKD" >
    <Fields>
      <Float name="DamageMult" />
      <Float name="Chance" />
      <FormLink name="Spell" refName="ASpell" />
      <Enum name="Flags" enumName="AttackData.Flag" byteLength="4" />
      <Float name="AttackAngle" />
      <Float name="StrikeAngle" />
      <Float name="Stagger" />
      <Float name="Knockdown" />
      <Float name="RecoveryTime" />
      <Float name="ActionPointsMult" />
      <Int32 name="StaggerOffset" />
    </Fields>
  </Object>
</Loqui>
